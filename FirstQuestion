package in.ineuron.main; 
class Reverse{
	public void stringReverse(String s) {
		String s1="";
		for(int i=0;i<s.length();i++) {
			s1=s.charAt(i)+s1;
		} 
		System.out.println(s1);
	}
}

public class ForPract {

	public static void main(String[] args) {
		String s="iNeuron";
		Reverse obj=new Reverse();
		obj.stringReverse(s);
	}
}
